        <record id="seq_gmao_pm_alert" model="ir.sequence">
            <field name="name">tms.gmao pm alert</field>
            <field name="code">tms.gmao.pm.alert</field>
            <field name="prefix">ALT-</field>
            <field name="padding">5</field>
