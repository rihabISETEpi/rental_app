		<!-- Sequences Yield-->

        <record id="seq_product_price_yieldr" model="ir.sequence">
            <field name="name">Tarif Yield</field>
            <field name="code">product.price.yield</field>
            <field name="prefix">YLD</field>
            <field name="padding">8</field>
        </record>
        
		<!-- Sequences pour la consommation kilométrique mensuelle-->

        <record id="seq_fleet_vehicle_contract_odometer" model="ir.sequence">
            <field name="name">Consommation kilométrique</field>
            <field name="code">fleet.vehicle.contract.odometer</field>
