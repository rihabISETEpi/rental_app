            <field name="padding">8</field>
        </record>
        
		<!-- Sequences pour les mouvements-->

        <record id="seq_fleet_vehicle_move" model="ir.sequence">
            <field name="name">fleet vehicle move</field>
            <field name="code">fleet.vehicle.move</field>
