            <field name="prefix">TA-</field>
            <field name="padding">5</field>
        </record> 

<!-- ==================== tms_gmao_pm ===================== -->

        
        <record id="seq_gmao_pm" model="ir.sequence">
            <field name="name">tms.gmao pm</field>
            <field name="code">tms.gmao.pm</field>
            <field name="prefix">MP-</field>
            <field name="padding">5</field>
        </record> 
        
