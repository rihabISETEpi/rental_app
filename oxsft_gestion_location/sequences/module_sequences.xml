        <record id="seq_fleet_vehicle_contract_resa" model="ir.sequence">
            <field name="name">fleet vehicle contract reservation</field>
            <field name="code">fleet.vehicle.contract.resa</field>
            <field name="prefix">RES</field>
            <field name="padding">5</field>
        </record>
 

        <record id="seq_alert_link" model="ir.sequence">
            <field name="name">Liens</field>
            <field name="code">alert.link</field>
            <field name="prefix">LIEN-</field>
            <field name="padding">7</field>
        </record>

        <!-- Sequences for fleet.vehicle.modification-->

        <record id="seq_fleet_vehicle_modification" model="ir.sequence">
            <field name="name">fleet vehicle modification</field>
            <field name="code">fleet.vehicle.modification</field>
            <field name="prefix">Ref-modif</field>
            <field name="padding">5</field>
        </record>


<!-- ==================== tms_gmao_pm_model_template ===================== -->

        
        <record id="seq_gmao_pm_model_template" model="ir.sequence">
            <field name="name">tms.gmao pm.model.template</field>
