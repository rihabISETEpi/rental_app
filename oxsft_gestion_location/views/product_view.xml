            <field name="model">product.product</field>
            <field name="inherit_id" ref="product.product_normal_form_view" />
            <field name="priority" eval="1" />
            <field name="arch" type="xml">
				<xpath expr="/form/sheet/notebook/page[position()=last()]" position="after">
					<page string="ParticularitÃ©s">
						<group>
							<group>
								<field name="vente" />
								<field name="achat" />
								<field name="historique" string="Historique"/>
								<field name="prorata" />
							</group>
							<group string="Prestations">
								<field name="prestation_ok" />
